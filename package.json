{
  "name": "at2",
  "version": "1.0.0",
  "description": "CLI tool for automating QIMA project releases with plugin support",
  "main": "dist/src/index.js",
  "bin": {
    "at2": "dist/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
